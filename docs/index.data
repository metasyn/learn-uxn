@rand ( -- 1 )

	.seed/x LDZ #00 SWP DUP2 MUL2
	.seed/w LDZ2 .seed/s LDZ2 ADD2
	DUP2 .seed/w STZ2
	ADD2
	#04 SFT SWP #40 SFT ADD
	DUP .seed/x STZ

